		<List src="image/BookService.png" items="{oAppModel>/vehicles}" class="VehicleListItemCSS UserInfoHeaderAlign" press="onSelectableItemPress">
			<CustomListItem width="60%">
				<HBox class="VehicleListVBCSS sapUiSmallMarginBeginEnd sapUiSmallMarginTopBottom" justifyContent="SpaceBetween"
					visible="{parts: ['oAppModel>isDefault'], formatter:'com.sap.alj.sca.ALJ_SCA.util.formatter.setDefaultVehicleStyle'}">
					<VBox width="50%" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom vehiclListAlignCSS">
						<Image src="{oAppModel>src}" class="aljCarListLogos"/>
						<Text text="{oAppModel>model} {oAppModel>year}" class="aljModelTextStyle aljCarListText"/>
						<Text text="{oAppModel>vin}" class="aljCarListText"/>
						<HBox class="aljTextBlock">
							<Text text="{i18n>LastServiceDate}" class="aljCarListText"/>
							<Text text="{oAppModel>lastServiceDate}" class="aljCarListText aljRedColorStyle sapUiTinyMarginBegin"/>
						</HBox>
					</VBox>
					<VBox width="30%" justifyContent="Center" alignItems="Center" class="sapUiTinyMarginBeginEnd">
						<Image src="image/Edit.png" class="aljMyCarsEditIcon"/>
						<Button text="{parts: ['oAppModel>isDefault'], formatter:'com.sap.alj.sca.ALJ_SCA.util.formatter.getDefaultVBtnText'}"
							press="onMakeDefaultCar"/>
					</VBox>
				</HBox>
			</CustomListItem>
		</List>
	</ScrollContainer>
</core:FragmentDefinition>