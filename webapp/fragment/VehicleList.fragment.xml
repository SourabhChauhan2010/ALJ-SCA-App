		<Image src="image/vehicleInfo.png" class="sapUiTinyMarginEnd"/>
		<Text text="{i18n>VehicleInformation}" class="aljHomeText"/>
	</HBox>
	<!--</contentMiddle>-->
	<!--</Bar>-->
	<ScrollContainer height="100%" width="100%" horizontal="true" focusable="true">
