<mvc:View controllerName="com.sap.alj.sca.ALJ_SCA.controller.Home" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" displayBlock="true"
	xmlns="sap.m">
	<VBox>
		<Carousel id="offersCarousel" pages="{oAppModel>/aOffers}" loop="true">
			<pages>
				<Image press="onOfferPress" height="200px" src="{oAppModel>imageSource}"/>
			</pages>
		</Carousel>
	</VBox>
	<VBox>
		<Label text="Recommended for you" class="styleStatusTitle sapUiSmallMarginBegin"/>
		<ScrollContainer height="100%" width="100%" horizontal="true" focusable="true">
			<HBox items="{oAppModel>/aRecommendationsHome}" class="sapUiSmallMarginBeginEnd">
				<CustomListItem class="styleRecommendedBox" type="Active">
					<VBox alignItems="Center">
						<Image src="{oAppModel>model}" width="6rem"/>
						<Label text="{oAppModel>serviceType}" class="sapUiTinyMarginBottom styleServType"/>
						<Image src="{oAppModel>src}" width="6rem"/>
						<Label text="{oAppModel>sar2}" class="styleCAR2"/>
						<Text text="{oAppModel>sar1}" class="sapUiTinyMarginBottom styleCAR1"/>
						<Button text="Book" class="aljRedBtnStyle" press="onBookService"/>
					</VBox>
				</CustomListItem>
			</HBox>
		</ScrollContainer>
	</VBox>
	<CustomListItem press="onBookService" class="styleStatusBox" type="Active">
		<HBox justifyContent="SpaceBetween">
			<VBox binding="{oAppModel>/oServiceStatus}" class="sapUiSmallMarginBegin">
				<Text text="Service Status" class="styleStatusTitle sapUiTinyMarginBottom sapUiTinyMarginTop"/>
				<Text text="{oAppModel>model}" class="sapUiTinyMarginBottom"/>
				<Text text="PDT: {oAppModel>time}" class="sapUiTinyMarginBottom"/>
				<Text text="Status: {oAppModel>status}"/>
			</VBox>
			<VBox class="sapUiMediumMarginEnd">
				<Image height="107px"
					src="data:image/png;base64,"/>
			</VBox>
		</HBox>
	</CustomListItem>
	<VBox class="sapUiTinyMarginTop">
		<Label text="Services" class="styleStatusTitle sapUiSmallMarginBegin sapUiTinyMarginBottom"/>
		<ScrollContainer height="100%" width="100%" horizontal="false" focusable="true">
			<VBox items="{oAppModel>/aServices}" class="sapUiSmallMarginBeginEnd" id="idVBServiceListItems">
				<CustomListItem press="NavtoServices" class="styleServiceBox sapUiTinyMarginBottom" type="Active">
					<FlexBox justifyContent="SpaceBetween" alignItems="Center">
						<Image src="{oAppModel>src}" width="40px" height="40px" class="sapUiTinyMarginBegin" press="NavtoServices" tooltip="{oAppModel>name}"/>
						<Label text="{oAppModel>name}" class="styleLabel"/>
						<VBox class="styleArrowBox">
							<Image src="image/arrow.png" width="14px" height="26px" press="onMyService" class="sapUiTinyMarginEnd"/>
						</VBox>
					</FlexBox>
				</CustomListItem>
			</VBox>
		</ScrollContainer>
	</VBox>
</mvc:View>