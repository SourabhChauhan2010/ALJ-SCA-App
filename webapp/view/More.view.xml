<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.sap.alj.sca.ALJ_SCA.controller.More"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout">
	<VBox class="aljBoxContentStyle">
		<VBox renderType="Bare">
			<l:Grid containerQuery="true" defaultSpan="XL2 L3 M4 S6" class="aljMoreTilesStyle sapUiMediumMarginTop">
				<CustomListItem type="Active" press="openSafetyMaintenance">
					<Text text="{i18n>SafetyMaintenanceTips}"/>
				</CustomListItem>
				<CustomListItem type="Active" press="onSelectableItemPress">
					<Text text="{i18n>DefaultServiceCenter}"/>
				</CustomListItem>
				<CustomListItem type="Active" press="onSelectableItemPress">
					<Text text="{i18n>DefaultVehicle}"/>
				</CustomListItem>
			</l:Grid>
		</VBox>
	</VBox>
</mvc:View>