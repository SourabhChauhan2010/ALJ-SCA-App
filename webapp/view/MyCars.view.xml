<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.sap.alj.sca.ALJ_SCA.controller.MyCars"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:f="sap.ui.layout.form">
	<VBox class="aljBoxContentStyle">
		<VBox class="sapUiSmallMarginTop">
			<core:Fragment fragmentName="com.sap.alj.sca.ALJ_SCA.fragment.VehicleList" type="XML"/>
		</VBox>
		<VBox alignItems="Center" justifyContent="Center">
			<Button text="{i18n>AddAnother}" class="aljBlackBtnStyleRev sapUiSmallMarginBottom" width="18rem" height="3.5rem" press="onAddVehiclePress"/>
			<Button text="{i18n>Next}" class="aljRedBtnStyle" width="18rem" height="3.5rem"/>
		</VBox>
	</VBox>
	<core:Fragment fragmentName="com.sap.alj.sca.ALJ_SCA.fragment.DesktopFooter" type="XML"/>
</mvc:View>