	"authenticationMethod": "none",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/ALJ_SCA/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "^/sap/opu/odata/",
		"target": "/",
		"destination": "ERPDEV"
	}, {
		"source": "^/SBA_book_a_service/",
		"target": "/",
		"destination": "SBA_book_a_service"
	}, {
		"source": "^/SCA_JAVA/",
		"target": "/",
		"destination": "SCA_JAVA"
	}, {
		"source": "^/Java_Service/",
		"target": "/",
		"destination": "Java_Service"
	}, {
		"source": "^/SCA_CRM/",
		"target": "/",
		"destination": "SCA_CRM"
	}]
}