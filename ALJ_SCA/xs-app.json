{
	"welcomeFile": "/ALJ_SCA/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/ALJERP-DEV/(.*)$",
		"target": "$1",
		"authenticationType": "none",
		"destination": "ERPDEV",
		"csrfProtection": false
	}, {
		"source": "^/SCA_CRM/(.*)$",
		"target": "$1",
		"authenticationType": "none",
		"destination": "SCA_CRM",
		"csrfProtection": false
	}, {
		"source": "^/SBA_book_a_service/",
		"target": "/",
		"destination": "SBA_book_a_service",
		"csrfProtection": false
	}, {
		"source": "^/SCA_JAVA/",
		"target": "/",
		"destination": "SCA_JAVA",
		"csrfProtection": false
	}, {
		"source": "^/Java_Service/",
		"target": "/",
		"destination": "Java_Service",
		"csrfProtection": false
	}, {
		"source": "^/ALJ_SCA/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}]
}